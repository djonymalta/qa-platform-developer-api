{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": [
        "code",
        "data",
        "message",
        "success"
    ],
    "properties": {
        "code": {
            "type": "integer"
        },
        "data": {
            "type": "object",
            "required": [
                "AccessToken",
                "ExpiresIn",
                "IdToken",
                "RefreshToken",
                "TokenType"
            ],
            "properties": {
                "AccessToken": {
                    "type": "string"
                },
                "ExpiresIn": {
                    "type": "integer"
                },
                "IdToken": {
                    "type": "string"
                },
                "RefreshToken": {
                    "type": "string"
                },
                "TokenType": {
                    "type": "string"
                }
            }
        },
        "message": {
            "type": "string"
        },
        "success": {
            "type": "boolean"
        }
    }
}